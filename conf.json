{
  "CONTEST_DIRECTORY": "contest_files",
  "SOURCE_DIRECTORY": "source_files",
  "EXECUTABLE_DIRECTORY": "executables",
  "TIMEOUT": "2",
  "ENV": {
    ".c": {
      "compile": "gcc -static -fno-optimize-sibling-calls -fno-strict-aliasing -fno-asm -lm -s -O2 -m32 -o {problem_id} {problem_id}.c",
      "execute": "./{problem_id}"
    },
    ".cpp": {
      "compile": "g++ -static -fno-optimize-sibling-calls -fno-strict-aliasing -lm -s -x c++ -O2 -std=c++11 -D__USE_MINGW_ANSI_STDIO=0 -m32 -o {output_file} {source_file}",
      "execute": "./{output_file}"
    },
    ".py": {
      "execute": "python {source_file}"
    },
    ".java": {
      "compile": "javac -cp '.;*' {problem_id}.java",
      "execute": "java -Djava.security.manager -Djava.security.policy=java.policy -Xmx512M -Xss64M -Duser.language=en -Duser.region=US -Duser.variant=US {problem_id}"
    },
    ".scala": {
      "compile": "fsc -cp '.;*' {problem_id}.scala",
      "execute": "JAVA_OPTS='-Djava.security.policy=java.policy -Xmx512M -Xss64M -Duser.language=en -Duser.region=US -Duser.variant=US' scala {problem_id}"
    },
    ".rb": {
      "execute": "ruby {problem_id}.rb"
    },
    ".go": {
      "compile": "go build {problem_id}.go",
      "execute": "./{problem_id}"
    },
    ".hs": {
      "compile": "ghc --make -O -o {problem_id} {problem_id}.hs",
      "execute": "./{problem_id}"
    }
  }
}
